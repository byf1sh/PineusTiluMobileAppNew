<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingWindowsPageActivity"
    android:background="@drawable/bgsettingwindow">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/buttonname_profilepage"
        android:layout_width="340dp"
        android:layout_height="wrap_content"
        android:textAlignment="viewStart"
        android:background="@drawable/buttonshape"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        >
        <ImageView
            android:id="@+id/profileicon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/name_icon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="24.41dp"/>
        <TextView
            android:id="@+id/changepasswordprofile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Change Password"
            android:textColor="#263238"
            android:textSize="18sp"
            android:fontFamily="@font/ralewaysemibold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/profileicon"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            />
        <TextView
            android:id="@+id/lastchange"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Last Change : xx/xx/xx"
            android:textColor="#63767E"
            android:layout_marginStart="12.5dp"
            android:paddingBottom="12.5dp"
            android:fontFamily="@font/ralewaymedium"
            android:textSize="16sp"
            app:layout_constraintTop_toBottomOf="@id/changepasswordprofile"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/profileicon"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/buttonaddress_profilepage"
        android:layout_width="340dp"
        android:layout_height="wrap_content"
        android:textAlignment="viewStart"
        android:layout_marginTop="10dp"
        android:background="@drawable/buttonshape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonname_profilepage"
        >
        <ImageView
            android:id="@+id/homeicon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/home_light22"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="24.41dp"/>
        <TextView
            android:id="@+id/changeaddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Change Address"
            android:textColor="#263238"
            android:textSize="18sp"
            android:fontFamily="@font/ralewaysemibold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/homeicon"
            app:layout_constraintBottom_toTopOf="@id/changeaddress"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            />
        <TextView
            android:id="@+id/address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Jalan Mars Dirgahayu no.28"
            android:textColor="#63767E"
            android:layout_marginStart="12.5dp"
            android:paddingBottom="12.5dp"
            android:fontFamily="@font/ralewaymedium"
            android:textSize="16sp"
            app:layout_constraintTop_toBottomOf="@id/changeaddress"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/homeicon"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/buttonnumber_profilepage"
        android:layout_width="340dp"
        android:layout_height="wrap_content"
        android:textAlignment="viewStart"
        android:layout_marginTop="10dp"
        android:background="@drawable/buttonshape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonaddress_profilepage"
        >
        <ImageView
            android:id="@+id/dialicon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/dial_icon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="24.41dp"/>
        <TextView
            android:id="@+id/contactnumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Change Contact Number"
            android:textColor="#263238"
            android:textSize="18sp"
            android:fontFamily="@font/ralewaysemibold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/dialicon"
            app:layout_constraintBottom_toTopOf="@id/contactnumber"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            />
        <TextView
            android:id="@+id/number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="+62 82215437001"
            android:textColor="#63767E"
            android:layout_marginStart="12.5dp"
            android:paddingBottom="12.5dp"
            android:fontFamily="@font/ralewaymedium"
            android:textSize="16sp"
            app:layout_constraintTop_toBottomOf="@id/contactnumber"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/dialicon"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/buttonemail_profilepage"
        android:layout_width="340dp"
        android:layout_height="wrap_content"
        android:textAlignment="viewStart"
        android:layout_marginTop="10dp"
        android:background="@drawable/buttonshape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonnumber_profilepage"
        >
        <ImageView
            android:id="@+id/emailicon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/customer_icon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="24.41dp"/>
        <TextView
            android:id="@+id/changeemail_setting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Change Email"
            android:textColor="#263238"
            android:textSize="18sp"
            android:fontFamily="@font/ralewaysemibold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/emailicon"
            app:layout_constraintBottom_toTopOf="@id/profileName"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            />
        <TextView
            android:id="@+id/profileName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="rxmys@outlook.com"
            android:textColor="#63767E"
            android:layout_marginStart="12.5dp"
            android:paddingBottom="12.5dp"
            android:fontFamily="@font/ralewaymedium"
            android:textSize="16sp"
            app:layout_constraintTop_toBottomOf="@id/changeemail_setting"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/emailicon"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/buttoncustomersupport_profilepage"
        android:layout_width="340dp"
        android:layout_height="70dp"
        android:textAlignment="viewStart"
        android:layout_marginTop="10dp"
        android:background="@drawable/buttonshape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonemail_profilepage"
        >
        <ImageView
            android:id="@+id/csicon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/cs_icon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="24.41dp"/>
        <TextView
            android:id="@+id/customersupport"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Customer Support"
            android:textColor="#263238"
            android:textSize="18sp"
            android:fontFamily="@font/ralewaysemibold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/csicon"
            android:layout_marginStart="12dp"
            android:layout_marginTop="20dp"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/buttonFAQ_profilepage"
        android:layout_width="340dp"
        android:layout_height="70dp"
        android:textAlignment="viewStart"
        android:layout_marginTop="10dp"
        android:background="@drawable/buttonshape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttoncustomersupport_profilepage"
        >
        <ImageView
            android:id="@+id/faqicon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/faq_icon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="24.41dp"/>
        <TextView
            android:id="@+id/FAQ"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="FAQ"
            android:textColor="#263238"
            android:textSize="18sp"
            android:fontFamily="@font/ralewaysemibold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/faqicon"
            android:layout_marginStart="12dp"
            android:layout_marginTop="20dp"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>



